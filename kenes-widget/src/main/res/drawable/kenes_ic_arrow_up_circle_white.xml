<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="oval">
            <solid android:color="@color/kenes_white" />

            <size
                android:width="@dimen/kenes_send_message_icon_size"
                android:height="@dimen/kenes_send_message_icon_size" />

            <stroke android:color="@color/kenes_blue" android:width="2dp" />
        </shape>
    </item>

    <item
        android:drawable="@drawable/kenes_ic_arrow_up_blue"
        android:gravity="center"
        android:width="@dimen/kenes_send_message_icon_arrow_size"
        android:height="@dimen/kenes_send_message_icon_arrow_size" />

</layer-list>